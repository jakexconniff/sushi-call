{
    "hosting": {
      "rewrites": [
        {
          "source": "/token",
          "function": "token"
        },
        {
          "source": "/sendCall",
          "function": "sendCall"
        },
        {
          "source": "/callTwiml",
          "function": "callTwiml"
        }
      ],
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ]
    }
  }
  